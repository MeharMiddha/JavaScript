// Number
let a=23;
console.log('The value of a is : ',a);
console.log('The type of a is : ',typeof(a)); // here the type of a will be number

// String
a='23';
console.log('The value of a is : ',a);
console.log('The type of a is : ',typeof(a)); // here the type of a will be String

// now if we add something to a it will add up in the form of string
a=a+1;
console.log('The value of a is :',a); //231 will come as a string


// Boolean
a=false;
console.log('The value of a is : ',a);
console.log('The type of a is : ',typeof(a)); // here the type of a will be boolean

// Undefined
a=undefined;

console.log('The value of a is : ',a);
console.log('The type of a is : ',typeof(a)); // here the type of a will be NotaNumber

// Null
a=null;
console.log('The value of a is : ',a);
console.log('The type of a is : ',typeof(a)); // here the type of a will be null

